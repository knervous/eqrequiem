    "typescript-eslint": "^8.24.1",
    "vercel": "^41.4.1",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-ssr": "^0.4.142",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-plugin-vercel": "^9.0.5"
  }
